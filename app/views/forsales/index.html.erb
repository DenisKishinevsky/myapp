<h1>Listing forsales</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Gmaps</th>
      <th>City</th>
      <th>Postal code</th>
      <th>Province</th>
      <th>Description</th>
      <th>House type</th>
      <th>Style</th>
      <th>Year build</th>
      <th>Lot size</th>
      <th>Size</th>
      <th>Lot type</th>
      <th>Bedrooms</th>
      <th>Bathroom</th>
      <th>Ext finish</th>
      <th>Heating type</th>
      <th>Heating fuel</th>
      <th>Cooling type</th>
      <th>Water</th>
      <th>Parking type</th>
      <th>Total parking space</th>
      <th>Liv room</th>
      <th>Din room</th>
      <th>Kitchen</th>
      <th>Part bathroom</th>
      <th>Eating area</th>
      <th>Mas bedroom</th>
      <th>Bedroom 2</th>
      <th>Bedroom 3</th>
      <th>Bedroom 4</th>
      <th>Bedroom 5</th>
      <th>Bathroom</th>
      <th>Bathroom 2</th>
      <th>Bathroom 3</th>
      <th>Laundry room</th>
      <th>Rec room</th>
      <th>Mls</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @forsales.each do |forsale| %>
      <tr>
        <td><%= forsale.name %></td>
        <td><%= forsale.address %></td>
        <td><%= forsale.longitude %></td>
        <td><%= forsale.latitude %></td>
        <td><%= forsale.gmaps %></td>
        <td><%= forsale.city %></td>
        <td><%= forsale.postal_code %></td>
        <td><%= forsale.province %></td>
        <td><%= forsale.description %></td>
        <td><%= forsale.house_type %></td>
        <td><%= forsale.style %></td>
        <td><%= forsale.year_build %></td>
        <td><%= forsale.lot_size %></td>
        <td><%= forsale.size %></td>
        <td><%= forsale.lot_type %></td>
        <td><%= forsale.bedrooms %></td>
        <td><%= forsale.bathroom %></td>
        <td><%= forsale.ext_finish %></td>
        <td><%= forsale.heating_type %></td>
        <td><%= forsale.heating_fuel %></td>
        <td><%= forsale.cooling_type %></td>
        <td><%= forsale.water %></td>
        <td><%= forsale.parking_type %></td>
        <td><%= forsale.total_parking_space %></td>
        <td><%= forsale.liv_room %></td>
        <td><%= forsale.din_room %></td>
        <td><%= forsale.kitchen %></td>
        <td><%= forsale.part_bathroom %></td>
        <td><%= forsale.eating_area %></td>
        <td><%= forsale.mas_bedroom %></td>
        <td><%= forsale.bedroom_2 %></td>
        <td><%= forsale.bedroom_3 %></td>
        <td><%= forsale.bedroom_4 %></td>
        <td><%= forsale.bedroom_5 %></td>
        <td><%= forsale.bathroom %></td>
        <td><%= forsale.bathroom_2 %></td>
        <td><%= forsale.bathroom_3 %></td>
        <td><%= forsale.laundry_room %></td>
        <td><%= forsale.rec_room %></td>
        <td><%= forsale.mls %></td>
        <td><%= forsale.price %></td>
        <td><%= link_to 'Show', forsale %></td>
        <td><%= link_to 'Edit', edit_forsale_path(forsale) %></td>
        <td><%= link_to 'Destroy', forsale, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

